{
  "expo": {
    "name": "Mouni",
    "slug": "Mouni",
    "version": "1.0.1",
    "orientation": "portrait",
    "privacy": "public",
    "icon": "./app/assets/app-icon.png",
    "splash": {
      "image": "./app/assets/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
